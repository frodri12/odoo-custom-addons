<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit" model="ir.ui.view">
        <field name="name">account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">

            <xpath expr="//page[3]" position="after">
                <page name="DNIT">
                    <group>
                        <group>
                            <field name="l10n_py_dnit_ws_response_codres" />
                        </group>
                        <group>
                            <field name="l10n_py_dnit_ws_response_msgres" />
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="l10n_py_dnit_ws_response_estres" />
                            <field name="l10n_py_dnit_ws_response_fecproc" />
                            <field name="l10n_py_dnit_ws_random_code" />
                        </group>
                        <group>
                            <field name="l10n_py_dnit_ws_response_cdc" />
                            <field name="l10n_py_dnit_ws_response_protaut" />
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="l10n_py_dnit_ws_request_json" />
                        </group>
                        <group>
                            <field name="l10n_py_dnit_ws_response_json" />
                        </group>
                    </group>
                </page>
            </xpath>

        </field>
    </record>
</odoo>